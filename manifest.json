{
   "manifest_version": 2,
   "name": "Debug SharePoint Javascript sources",
   "short_name": "SP JS Debug",
   "description": "This extension gives access to the debug-version of the SharePoint Javascript source files",
   "version": "1.1",
   "icons": {
      "128": "/icons/icon128.png",
      "16": "/icons/icon16.png",
      "48": "/icons/icon48.png"
   },
   "background": {
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_icon": "/icons/icon19_disabled.png"
   },
   "permissions": [
       "webRequest",
       "webRequestBlocking",
       "https://*.sharepoint.com/",
       "https://*.sharepointonline.com/"
    ]
}
